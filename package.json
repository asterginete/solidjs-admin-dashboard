    "passport-local": "1.0.0",
    "express-session": "1.17.2"
  },
  "devDependencies": {
    "@types/node": "16.7.1",
    "@types/express": "4.17.11",
    "@types/mongoose": "5.11.97",
    "typescript": "4.4.2",
    "solid-scripts": "1.0.0",
    "concurrently": "6.2.0"
  }
